{
	"manifest_version": 2,
	"name": "findFailures",
	"description": "This extension finds test failures in TTM/TNG logs.",
	"version": "1.0",

 	"icons": {
    "16":   "images/icon16.png",
    "48":   "images/icon48.png",
    "128":  "images/icon128.png"
  	},

  	"permissions": [
        "tabs",
        "http://gwyedgettm.cisco.com/blackbox/*",
        "http://ttm.rusclabs.cisco.com/blackbox/*",
        "storage"
  	],

  	"browser_action": {
    	"default_icon": "images/icon.png",
    	"default_popup": "html/popup.html"
  	},

  	"background": {
    	"scripts": ["js/background.js"],
    	"persistent": false
    }
}
